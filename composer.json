{
    "name": "bazoonchik/critical-alerts",
    "description": "Works with monolog to forward selected log calls to handlers such as Telegram or Sentry.",
    "type": "symfony-bundle",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "4.3.*",
        "sensio/framework-extra-bundle": "^5.2",
        "symfony/yaml": "4.3.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/http-client": "^4.3",
        "ext-json": "*"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "bazoonchik\\CriticalAlertsBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "bazoonchik\\CriticalAlertsBundle\\Tests\\": "tests"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.3.*"
        }
    },
    "support": {
        "name": "Andy W",
        "email": "bazoonchik@gmail.com"
    }
}
